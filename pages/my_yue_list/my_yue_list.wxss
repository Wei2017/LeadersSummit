.fixed_right{
  width: 120rpx;
  height: 80rpx;
  line-height: 80rpx;
  position: fixed;
  right: 0;
  bottom: 100rpx;
  border-top-left-radius: 50rpx;
  border-bottom-left-radius: 50rpx;
  background-color: #ecb700;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.fixed_right>text{
  font-size: 30rpx;
  color: #fff;
  margin-left: 16rpx;
}
.fixed_right>image{
  width: 26rpx;
  height: 24rpx;
  margin-left: 8rpx;
  margin-top: 5rpx;
}