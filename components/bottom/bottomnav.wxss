/* components/bottom/bottomnav.wxss */
.nav{
  width: 100%;
  height: 90rpx;
  padding-top:15rpx; 
  padding: 0rpx 68rpx 0rpx 64rpx;
  box-sizing: border-box;
  position: relative;
}
.navli{
  width: 75rpx;
  text-align: center;
  float: left;
}
.navli:first-of-type{
  margin-right:41rpx; 
}
.navli:nth-of-type(3){
  float: right; 
  margin-left: 52rpx;
}
.lastli{
  float: right; 
}
.navli_img_one{
  width: 36rpx;
  height: 38rpx;
  margin-bottom:8rpx; 
  margin-top:15rpx; 
  display: inline-block;
}
.navli_img_two{
  width: 36rpx;
  height: 43rpx;
  margin-bottom:9rpx; 
  margin-top:11rpx; 
  display: inline-block;
}
.navli_img_three{
  width: 43rpx;
  height: 40rpx;
  margin-bottom:9rpx; 
  margin-top:14rpx; 
  display: inline-block;
}
.navli_img_four{
  width: 40rpx;
  height: 40rpx;
  margin-bottom:9rpx; 
  margin-top:14rpx; 
  display: inline-block;
}
.navli_name{
  display: inline-block;
  font-size: 24rpx;
  color: #aaaaaa;
}
.navli_name_color{
  display: inline-block;
  font-size: 24rpx;
  color: #ecb700;
}
.sign{
  position: absolute;
  bottom: -6rpx;
  left: 295rpx;
  width: 164rpx;
  height: 164rpx; 
  display: block;
  z-index: 999;
  border-radius:50%;
  background: #fff;
  box-sizing: border-box;
  text-align: center;
}
.sign_img{
  width: 148rpx;
  height: 148rpx;
}
.banyuan{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  width: 100%;
  height: 182rpx;
}